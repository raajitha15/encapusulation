/*wap to enter a number and check whether its a SPY number or not
spy number: if the product of digit is equal to sum of digits.then the 
number is said to be a spy number.*/
import java.util.Scanner;
class	Spy  
{
	public static void main(String[] args) 
	{
		int digit,sum=0,product=1,num;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number");
		num=sc.nextInt();
        do
		{
			digit=num%10;
			sum=sum+digit;
			product=product*digit;
			num=num/10;
		}
		while (num!=0);
		if(sum==product)
		{
			System.out.println("it is a spy number");
		}
			else
		{
				System.out.println("it is not a spy number");
		}

	}
}
