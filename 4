/*WAP to enter the number whether the number is fibonnaci or not
fibonnaci : each succeeding term is sum of the two immediately preceding*/

class  Fibonnaci_Number
{
	public static void main(String[] args) 
	{
		int a=0,b=1,sum=0;
		System.out.println(a);
		for(int i=0;i<=10;i++)
		{
			sum=a+b;
		    b=a;
			a=sum;
			System.out.println(sum);
		}

		
	}
}
