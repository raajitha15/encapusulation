//encapsulation program
class  AnyAccount
{
	private int x=12000;
	public int modiGetData()
	{
		return x;
	}
}
class CommonPeople
{

	public static void main(String[] args) 
	{
		AnyAccount aa1=new AnyAccount();
		System.out.println(aa1.x);
	}
}
class Modi2
{
	public static void main(String[]args)
	{
		AnyAccount a1=new AnyAccount();
		System.out.println(a1.modiGetData());
	}

}
